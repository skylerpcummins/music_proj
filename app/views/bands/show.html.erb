<%= button_to "Delete this band", band_url, method: :delete %>

<h3><%= "#{@band.name}" %></h3>

<ul>
<% unless @band.albums.nil? %>
  <% @band.albums.each do |album| %>
  <li>
    <%= link_to "#{album.title}", album_url(album)%>
  </li>
  <% end %>
<% end %>
</ul>

<%= link_to "Edit this band", edit_band_url(@band) %>
<br>
<%= link_to "Add album for this band", new_band_album_url(@band) %>
<br>
<%= link_to "Back to bands", bands_url %>
